class Grid:
	var data : PackedFloat32Array
	
	var resolution : int
	
	var height : int
	var width : int
	var length : int
	
	func _init(resolution : int, height : int, width : int, length : int):
		self.height = height
		self.width = width
		self.length = length
		self.resolution = resolution
		
		data.resize(height * width * length)
		data.fill(1.0)
	func read(x : int, y : int, z:int) -> float:
		return data[x + length * (y + height * z)]
	func write(x : int, y : int, z : int, value : float):
		data[x + length * (y + height * z)] = value;

const POINTS = [
	Vector3i(0, 0, 0),
	Vector3i(0, 0, 1),
	Vector3i(1, 0, 1),
	Vector3i(1, 0, 0),
	Vector3i(0, 1, 0),
	Vector3i(0, 1, 1),
	Vector3i(1, 1, 1),
	Vector3i(1, 1, 0),
]

const EDGES = [
	Vector2i(0, 1),
	Vector2i(1, 2),
	Vector2i(2, 3),
	Vector2i(3, 0),
	Vector2i(4, 5),
	Vector2i(5, 6),
	Vector2i(6, 7),
	Vector2i(7, 4),
	Vector2i(0, 4),
	Vector2i(1, 5),
	Vector2i(2, 6),
	Vector2i(3, 7),
]

func scalar_field(x:float, y:float, z:float):
	return (x * x + y * y + z * z)/60.0

