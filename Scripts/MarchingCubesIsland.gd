func march_cubes(x:int, y:int, z:int, vf:Grid, verts:PackedVector3Array):
	var tri : Array = get_triangulation(x, y, z, vf)
	for edge_index in tri:
		if edge_index < 0: break
		var point_indeces : Vector2 = EDGES[edge_index]
		var p0 : Vector3 = POINTS[point_indeces.x]
		var p1 : Vector3 = POINTS[point_indeces.y]
		var pos_a = Vector3(x + p0.x, y + p0.y, z+p0.z)
		var pos_b = Vector3(x + p1.x, y + p1.y, z+p1.z)
		
		var pos : Vector3 = calculate_interp(pos_a, pos_b, vf)
		verts.append(pos)

func calculate_interp(pos_a : Vector3, pos_b : Vector3, vf : Grid) -> Vector3:
	#https://stats.stackexchange.com/questions/70801/how-to-normalize-data-to-0-1-range
	var val_a : float = vf.read(pos_a.x, pos_a.y, pos_a.z)
	var val_b : float = vf.read(pos_b.x, pos_b.y, pos_b.z)
	var norm : float = (isolevel - val_a) / (val_b - val_a)
	return pos_a + norm * (pos_b - pos_a)

func get_triangulation(x:int, y : int, z : int, vf:Grid) -> Array:
	var idx = 0b00000000
	idx |= int(vf.read(x,y,z) < isolevel) << 0
	idx |= int(vf.read(x,y,z + 1) < isolevel) << 1
	idx |= int(vf.read(x + 1,y,z + 1) < isolevel) << 2
	idx |= int(vf.read(x + 1,y,z) < isolevel) << 3
	idx |= int(vf.read(x,y + 1,z) < isolevel) << 4
	idx |= int(vf.read(x,y + 1,z + 1) < isolevel) << 5
	idx |= int(vf.read(x + 1,y + 1,z  + 1) < isolevel) << 6
	idx |= int(vf.read(x + 1,y + 1,z) < isolevel) << 7
	return triTable[idx]
